<!DOCTYPE html>
<html>
<head>
  <title>YourStory</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
 <p class="notice"><%= notice %></p>
 <p class="alert"><%= alert %></p>
<%if user_signed_in?%>
          <%=current_user.inspect%>
          <li><%=link_to "Edit Account",edit_user_registration_path(current_user)%> </li>
          <li><%= link_to "Signout", destroy_user_session_path,:method => :delete %></li>
          <%end%>
    
            <li><%= link_to "Sign Up with Twitter", user_omniauth_authorize_path(:twitter) %></li>
          </ul>
          <%=console%>
<%= yield %>


</body>
</html>
